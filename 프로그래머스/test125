using System;

public class Solution {
    public int solution(int[] sides) {
        Array.Sort(sides);
        int answer = 0;
        answer += sides[1] + sides[0] - sides[1];
        answer += sides[1] + sides[0] - sides[1] - 1;
        return answer;
    }
}
