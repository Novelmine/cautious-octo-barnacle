using System;
using System.Linq;

public class Solution {
    public int solution(int i, int j, int k) {
        int answer = 0; 
        char num = char.Parse(k.ToString());
        for(int p=i; p<=j; p++)
        {
            foreach (char c in p.ToString())
            {
                if(c == num)
                {
                    Console.WriteLine(c);
                    answer++;
                }
                
            }
        }
        return answer;
    }
}
