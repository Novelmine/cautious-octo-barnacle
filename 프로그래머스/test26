using System;

public class Solution {
    public long solution(long n) {
        long answer = 0;
        
        if(Math.Sqrt(n)%1 == 0)
        {
            answer = Convert.ToInt64(Math.Pow((Math.Sqrt(n)+1),2));
        }
        else
        {
            answer = -1;   
        }
        return answer;
    }
}
