https://school.programmers.co.kr/learn/courses/30/lessons/12912


public class Solution {
    public long solution(int a, int b) {
        long answer = 0;
        int c = 0;
        if(a > b)
        {
            c = a;
            a = b;
            b = c;
        }
        for(long i = a; i<=b; i++)
        {
            answer = answer + i;
        }
        return answer;
    }
}
