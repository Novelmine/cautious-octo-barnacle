using System;

public class Solution {
    public int solution(int[] numbers) {
        int answer = 0;
        for (int i = 0; i <=9; i++)
        {
            int k = i;
            if (Array.Exists(numbers, x  => x == k))
            {
            }
            else
            {
                answer += i;
            }

        }
        return answer;
    }
}
