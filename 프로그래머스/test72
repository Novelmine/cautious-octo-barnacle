https://school.programmers.co.kr/learn/courses/30/lessons/120843
using System;

public class Solution {
    public int solution(int[] numbers, int k) {
        int answer = 0;
        int q = 0;
        while(k > 1)
        {
            k--;
            q =q+2;
            if(q > numbers.Length-1)
            {
                q = q - numbers.Length ;
            }
        }
        answer = numbers[q];
        return answer;
    }
}
