https://school.programmers.co.kr/learn/courses/30/lessons/120892

using System;

public class Solution {
    public string solution(string cipher, int code) {
        string answer = "";
        for(int i=code-1; i<cipher.Length; i= i+code)
        {
            answer = answer + cipher[i];
        }
        return answer;
    }
}
