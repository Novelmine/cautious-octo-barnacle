https://school.programmers.co.kr/learn/courses/30/lessons/120815
using System;

public class Solution {
    public int solution(int n) {
        int answer = 1;
        for(int i=6; i<=n*6; i = i + 6)
        {
            if(i%n == 0)
            {
                break;
            }
            answer++;
        }
        return answer;
    }
}
