using System;
using System.Linq;
public class Solution {
    public string solution(string my_string) {
        string answer = "";
        my_string = my_string.ToLower();
        answer= new String(my_string.ToCharArray().OrderBy(x => x).ToArray());
        return answer;
    }
}
