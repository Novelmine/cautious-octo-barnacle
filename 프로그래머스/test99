https://school.programmers.co.kr/learn/courses/30/lessons/120903
using System;

public class Solution {
    public int solution(string[] s1, string[] s2) {
        int answer = 0;
        for(int i=0; i<s2.Length; i++)      {
            for(int k=0; k<s1.Length; k++)
                {
                if(s2[i] == s1[k])
                {
                   answer++; 
                }
            }
        }
        return answer;
    }
}
